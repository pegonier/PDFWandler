<VBox prefHeight="538.0" prefWidth="403.0" scaleShape="20.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.pegonier.pdfwandler.MainController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <AnchorPane prefHeight="540.0" prefWidth="363.0">
      <children>

          <Label fx:id="currentText" alignment="TOP_LEFT" layoutY="124.0" prefHeight="281.0" prefWidth="361.0" />
              <Region fx:id="spacer" prefHeight="0.0" prefWidth="350.0" />
         <ChoiceBox fx:id="DokChoice" layoutY="2.0" prefHeight="25.0" prefWidth="361.0" />
          <TextField fx:id="inputField" layoutY="33.0" onAction="#ANRsetzen" prefHeight="25.0" prefWidth="138.0" promptText="Auftragsnummer" />
         <TextField fx:id="inputField2" layoutY="58.0" onAction="#PIDsetzen" prefWidth="138.0" promptText="PID" />
         <TextField fx:id="inputField3" layoutY="83.0" onAction="#Name" prefHeight="25.0" prefWidth="196.0" promptText="Nachname^Vorname" />
         <TextField fx:id="inputField4" layoutX="220.0" layoutY="33.0" onAction="#Geburtsdatum" prefHeight="25.0" prefWidth="95.0" promptText="Geburtsdatum" />
