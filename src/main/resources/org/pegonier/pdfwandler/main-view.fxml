          <Button alignment="TOP_CENTER" layoutX="220.0" layoutY="83.0" onAction="#umwandeln" text="Umwandeln" />
          <Button fx:id="Properties" layoutX="325.0" layoutY="33.0" onAction="#Properties" text="*" />
         <ScrollPane fx:id="log" layoutX="1.0" layoutY="462.0" prefHeight="36.0" prefWidth="361.0" />
      </children>
   </AnchorPane>

    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <effect>
      <Blend />
   </effect>

</VBox>
